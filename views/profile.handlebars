<div>
	<a href = "/feed">Return to feed</a>
	<a href = "/logout">Logout</a>
	<ul>
		<li>Username: {{profile.username}}</li>
		<li>Name: {{profile.name}}</li>
		<li>Local: {{profile.local}}</li>
		<li>State: {{profile.state}}</li>
		<li>Region: {{profile.region}}</li>
	</ul>
	<h2>Posts</h2>
	<ul>
		{{#each uploads}}
			<article class = "post">
				<img src="data:image/png;base64, {{data}}" alt="Picc" />
			</article>
		{{/each}}
	</ul>
</div>